object pe2 {
  def fibonacci(int:Int) = {
    int match {
      case 0 => 1
      case 1 => 1
      case _ => fibonacci(int-1) + fibonacci(int-2)
    }
  }
  def solve = {
    val x = 0
    var sum = 0
    while (fibonacci(x) < 4000000) {
      fibonacci(x) % 2 match {
        case 0 => sum += fibonacci(x)
      }
      x++
    }
    sum
  }
